{
	"manifest_version": 3,
	"name": "FIKE® FIELD SERVICE Powered by VLR™",
	"version": "1.0.56",
	"key": "",
	"version_name": "1.0.56",
	"description": "Browser extension for editing Salesforce records offline",
	"icons": {
		"16": "/img/favicon.png",
		"128": "/img/logo.png"
	},
	"action": {
		"default_icon": "/img/favicon.png"
	},
	
	"background": {
		"service_worker": "background.js"
	},
	"host_permissions": [
		"https://fike--partial.lightning.force.com/*",
		"https://*.force.com/*",
		"https://*.salesforce.com/*"
	],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["contentScript.js"]
		}
	],
	"permissions": [
		"identity",
		"storage",
		"unlimitedStorage",
		"contextMenus",
		"notifications",
		"activeTab", 
		"scripting", 
		"tabs"
	],
	"web_accessible_resources": [
		{
			"resources": [
				"/config.json"
			],
			"matches": [
				"<all_urls>"
			]
		}
	],
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self';"
	}
}